{
    "name": "survos/deployment-bundle",
    "description": "Tools for deploying Survos projects to dokku",
    "keywords": [
        "symfony-ux",
        "symfony",
        "dev"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "symfony/form": "^7.3||^8.0",
        "symfony/console": "^7.3||^8.0",
        "symfony/process": "^7.3||^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/phpunit-bridge": "^7.3||^8.0",
        "symfony/twig-bundle": "^7.3||^8.0",
        "symfony/var-dumper": "^7.3||^8.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\DeploymentBundle\\": "src/"
        }
    }
}
